    ModIDS = {
    1860025727,    
    1094496852,
    1317454056,
    }


    cool = {
    417756911,
    }


function hintnames()
	for _,Player in pairs(game:GetService('Players'):GetChildren()) do
		if table.find(ModIDS, Player.UserId) then
			if Player.Character then
				if Player.Character.Parent.Name == 'Players' then
					Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[‚≠ê]' .. Player.DisplayName)
				end
			end
		elseif
			table.find(cool, Player.UserId) then
			if Player.Character then
				if Player.Character.Parent.Name == 'Players' then
					Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[üëë]' .. Player.DisplayName)
				end
			end
		if
			Player.Character then
			if Player.Character.Parent.Name == 'Players' then
				if not Player.Character.UpperTorso:FindFirstChild('OriginalSize') then
					Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[üö∞]' .. Player.DisplayName)
				end
			end
		end
	end
	end
	end
local success,err = pcall(hintnames)
return ModIDS
